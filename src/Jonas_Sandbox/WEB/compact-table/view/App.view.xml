<mvc:View controllerName="wt.controller.App" xmlns="sap.ui.table" xmlns:mvc="sap.ui.core.mvc" xmlns:u="sap.ui.unified" xmlns:c="sap.ui.core" xmlns:m="sap.m" class="sapUiSizeCompact">
  <m:Page showHeader="false" enableScrolling="false" class="sapUiContentPadding">
    <m:content>

     <Table rows="{/Events}" title="Products" selectionMode="MultiToggle" visibleRowCount="7">
        <columns>
          <Column width="11rem">
            <m:Label text="idEvent" />
            <template>
              <m:Text text="{idEvent}" />
            </template>
          </Column>
          <Column width="11rem">
            <m:Label text="idEventDef" />
            <template>
              <m:Input value="{idEventDef}" />
            </template>
          </Column>
          <Column width="6rem" hAlign="End">
            <m:Label text="timeStart" />
            <template>
              <m:Label text="{timeStart}" />
            </template>
          </Column>

        </columns>
      </Table>
    </m:content>
  </m:Page>
</mvc:View>

